# Vandal
# Delay minutes to check
vandalDelay=5
# Hour to check vandal 24 hour format
vandalHour=10
# Rss url
vandalURL=https://...

# General bot settings
# discord bot token
token=
# General text channel ID
defaultChannelID=
# Admin id for shutdown command usage format id1,id2,id3
adminID=
# This resets flags of daily checks minutes
resetDelay=1

# Commands
prefix=!
# Command that displays commands available for usage
helpCommand=help
# Detects text of and image using it's URL
ocrCommand=ocr
# Shutdown command
shutDownCommand=exit

# Countdown
# minutes
countDownRotate=1200
# java default java.util.Date class format
date=

# Help command
helpText=

# Ocr command tesseract lib route and so
dataPathTess=
# spa for spanish eng for english see more on tesseract doc
tessLang=

# Amazon Bot
# wishlist url must be public
wishlist=
# minutes 5-15 min so bot doesn't get detected
wishlistDelay=15
priceChangedMessage=
listChangedMessage=
botDetectedMessage=
urlTemplate=https://www.a...z.com/dp/

#SPRING
spring.main.banner-mode=false
logging.level.root=ERROR
spring.task.scheduling.pool.size=10

#Others
defaultErrorMessage=Error
